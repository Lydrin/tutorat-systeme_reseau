if [ $# -ne 2 ];then
	echo "Argumentation Fault!"
	exit 1
fi

if [ $1 = "aduqueno" ];then
	git config user.name "aduqueno"
	git config user.email "antoine.duquenoy@polytech-lille.net"
	git commit -m $2
	exit 0
fi

if [ $1 = "adurot" ];then
	git config user.name "adurot"
	git config user.email "anthony.durot@polytech-lille.net"
	git commit -m $2
	exit 0
fi
